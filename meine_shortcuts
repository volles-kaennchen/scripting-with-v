ls                                  --Ordner im Filemanager
ls ORDNERNAME                       --zeigt unterordner aus Ordner an
cd                                  --Homeverzeichniss
mkdir -p Uebung/Test1               --Erstellt einen ganzen Pfad
cd Uebung/Test1                     --Öffnet den erstellten Pfad
pwd                                 --zeigt pfad an
-----------------------------------------------------------------------------------
git init                            --Erstellt Git-Repository
git config --global user-name "diegoettliche"
git config --global user-email "vanessa.witzel@cloud.iad.de"
-----------------------------------------------------------------------------------
ll
touch index.html
nano index.html                     --Skript erstellen und bearbeiten
-----------------------------------------------------------------------------------
git checkout -- index.html

<html>
<body>
<h1>Ein erster Test</h1>
</body>
</html>                             --Mit STR X+Y+Enter beenden

strg + r - rückwärtssuche nach textmuster
git clone >> ordnerpfad mit \
get-module >> TOOLNAME
install-module >> TOOLNAME -FORCE
-----------------------------------------------------------------------------------
firefox index.html                  --Öffnet die zuvor erstellte HTML
cat index.html                      --Zeigt das Skript an
git checkout --index.html           --Skript wird auf den letzten Stand zurückgesetzt (Geht nur wenn ein Skript vorhanden ist)
cat index.html                      --Zeigt das Skript erneut an
