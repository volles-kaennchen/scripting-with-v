cat scripts/docker-provision.sh    --Zeigt Zertifikat an
-----------------------------------------------------------------------------------
openssl s_client -showcerts -connect gitlab:443 < /dev/null 2>/dev/null | sudo openssl x509 -outform PEM > /etc/ssl/certs/gitlab.pem
-- ruft zertifikat auf und gibt Zertifikat als .pem aus
